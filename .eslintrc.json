{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "max-len": "off",
    "@typescript-eslint/semi": "off",
    "no-new": "off",
    "object-curly-newline": "off",
    "@typescript-eslint/no-use-before-define": "off",
    "class-methods-use-this": "off"
  }
}
